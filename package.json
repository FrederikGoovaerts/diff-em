{
  "name": "diff-em",
  "version": "1.1.1",
  "author": "Frederik Goovaerts",
  "description": "A collection of utilities to calculate differences between JavaScript objects.",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json && tsc --project tsconfig.types.json",
    "prepublishOnly": "npm run build",
    "lint:check": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "format:check": "prettier --check src",
    "format:fix": "prettier --write src",
    "test": "jest",
    "test:coverage": "npm test -- --coverage"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.6.1",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "files": [
    "dist",
    "package.json",
    "README",
    "LICENSE"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "keywords": [
    "diff",
    "difference",
    "object",
    "added",
    "updated",
    "deleted",
    "jsonpath",
    "typescript"
  ],
  "repository": "github:FrederikGoovaerts/diff-em"
}
